{
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-cssxjs": "^0.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^15.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": ["cssxjs"]
  }
}
