.root
  min-height 100%
  +desktop()
    background-color $UI.colors.darkLighter
    justify-content center
    align-items center

.wrapper
  padding 2u
  width 100%
  border-radius 0.5u
  +desktop()
    width 50u
    background-color $UI.colors.white
    box-shadow $UI.colors.darkLighter 0 1u 1.5u
