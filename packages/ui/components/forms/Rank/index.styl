.droppable
  margin-top 1u
  flex-grow 1
  flex-shrink 1

.margin
  margin-top 1u

.draggable
  background-color: $UI.colors.white
  padding 1u
  border: 1px solid $UI.colors.darkLighter
  radius()

  +web()
    cursor pointer

.span
  flex-shrink 1
  flex-grow 1
  margin-left 2.5u
  justify-content center

.icon
  flex-shrink 0
  justify-content center
  margin-left 4u
