.item
  min-width 4u
  height 4u
  align-items center
  justify-content center
  padding-left 1u
  padding-right @padding-left

.page
  &.selected
    color var(--color-text-primary)

.status
  margin-left 1u
  margin-right @margin-left

.icon
  color var(--color-text-secondary)

  &.disabled
    color var(--color-text-placeholder)
