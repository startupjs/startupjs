{
  "name": "expoapp",
  "main": "expo-router/entry",
  "version": "0.60.0-canary.18",
  "type": "module",
  "scripts": {
    "start": "npm run check-redis && npx expo start",
    "check-redis": "grep -q \"REDIS_URL=\" .env.local || (echo \"!!!ERROR!!! Redis is required. Please see 'expoapp/README.md' for instructions.\" && exit 1)",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web",
    "build": "npx startupjs build",
    "start-production": "npx startupjs start-production",
    "test": "jest --watchAll",
    "startupjs:help": "npx startupjs --help",
    "postinstall": "npx startupjs postinstall",
    "worker": "npx startupjs start-worker"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.0",
    "@expo/vector-icons": "^15.0.3",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^7.1.6",
    "@startupjs/router": "^0.60.0-canary.7",
    "@startupjs/ui": "^0.60.0-canary.11",
    "@startupjs/worker": "^0.60.0-canary.7",
    "expo": "~54.0.27",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.10",
    "expo-image-picker": "~17.0.9",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "startupjs": "^0.60.0-canary.18"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "eslint": "^8.56.0",
    "eslint-config-cssxjs": "^0.2.0",
    "husky": "^4.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.14",
    "lint-staged": "^15.2.2",
    "react-test-renderer": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "cssxjs"
    ]
  }
}
