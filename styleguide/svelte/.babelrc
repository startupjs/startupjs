{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "regenerator": true }],
    ["@babel/plugin-proposal-class-properties", {"loose": true }],
    "@babel/proposal-optional-chaining",
    "@babel/proposal-nullish-coalescing-operator"
  ]
}
