{
  "name": "@startupjs/styleguide",
  "version": "0.34.7",
  "sideEffects": [],
  "private": true,
  "scripts": {
    "start": "startupjs start",
    "android": "react-native run-android && (npm run adb || true) && npm run log-android",
    "android-release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios-release": "react-native run-ios --configuration Release",
    "metro": "react-native start --reset-cache",
    "web": "startupjs web",
    "server": "startupjs server",
    "adb": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:3010 tcp:3010",
    "log-android": "hash ccze 2>/dev/null && npm run log-android-color || (echo \"WARNING! Falling back to plain logging. For colored logs install ccze - brew install ccze\" && react-native log-android)",
    "log-android-color": "react-native log-android | ccze -m ansi -C -o nolookups",
    "build": "startupjs build --async",
    "start-production": "startupjs start-production",
    "fonts": "startupjs fonts",
    "test:ios": "APP_ENV=detox startupjs test --ios --artifacts",
    "test:build:ios": "startupjs test --build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-community/datetimepicker": "^3.0.6",
    "@react-native-cookies/cookies": "^6.0.5",
    "@react-native-google-signin/google-signin": "^6.0.0",
    "@react-native-picker/picker": "^1.9.3",
    "@startupjs/2fa": "^0.34.2",
    "@startupjs/auth": "^0.34.2",
    "@startupjs/auth-apple": "^0.34.0",
    "@startupjs/auth-azuread": "^0.34.0",
    "@startupjs/auth-common": "^0.34.0",
    "@startupjs/auth-facebook": "^0.34.0",
    "@startupjs/auth-google": "^0.34.0",
    "@startupjs/auth-idg": "^0.34.0",
    "@startupjs/auth-linkedin": "^0.34.0",
    "@startupjs/auth-local": "^0.34.7",
    "@startupjs/docs": "^0.34.5",
    "@startupjs/mdx": "^0.34.5",
    "@startupjs/recaptcha": "^0.34.0",
    "@startupjs/scrollable-anchors": "^0.34.5",
    "@startupjs/ui": "^0.34.5",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "nconf": "^0.10.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "0.63.4",
    "react-native-collapsible": "1.5.2",
    "react-native-fbsdk": "^3.0.0",
    "react-native-pager-view": "^5.1.2",
    "react-native-restart": "^0.0.22",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^3.0.0",
    "react-native-webview": "10.10.2",
    "startupjs": "^0.34.0",
    "text-encoding-polyfill": "^0.6.7"
  }
}
